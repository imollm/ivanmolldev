---
import Repositories from './Repositories';

const gitHubAPIToken = import.meta.env.GITHUB_TOKEN
---

<section class='projects-component flex flex-col items-center w-full pb-10'>
    <div class='2xl:w-[70%] lg:w-[80%] w-[85%]'>
        <div class='flex flex-col pt-15 gap-4'>
            <h3 class='text-2xl'>
                Projects
            </h3>
            <Repositories client:load token={gitHubAPIToken}/>
        </div>
    </div>
</section>